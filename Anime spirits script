-- Functions for fruit Tab
function do25ksoul()
    while twentyFiveKsoul do
        local args = {
            [1] = "SoulsDealer",
            [2] = "G$25k 1 Soul"
        }
        
        game:GetService("ReplicatedStorage").Game.Remotes.ServerHandler:FireServer(unpack(args))
        
        wait(3)
    end
end

